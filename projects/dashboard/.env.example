# Dashboard Environment Configuration Example
# Copy this file to .env.local and update the values

# Base Configuration
NEXT_PUBLIC_BASE_URL=http://localhost
NEXT_PUBLIC_DASHBOARD_PORT=3005

# Development Services
NEXT_PUBLIC_CODE_SERVER_PORT=8889

# AI Services
NEXT_PUBLIC_AI_GATEWAY_PORT=8892
NEXT_PUBLIC_AI_AGENTS_PORT=8893
NEXT_PUBLIC_AI_KNOWLEDGE_PORT=8894
NEXT_PUBLIC_REALTIME_VOICE_PORT=8891
NEXT_PUBLIC_MUMUKO_PORT=8895

# Infrastructure Services
NEXT_PUBLIC_N8N_PORT=5678
NEXT_PUBLIC_SYNCTHING_PORT=8384
NEXT_PUBLIC_FILE_MANAGER_PORT=9000
NEXT_PUBLIC_NATS_PORT=8222

# Feature Flags
NEXT_PUBLIC_ENABLE_HEALTH_CHECK=true
NEXT_PUBLIC_HEALTH_CHECK_INTERVAL=30000
NEXT_PUBLIC_HEALTH_CHECK_TIMEOUT=5000

# Development Mode
NEXT_PUBLIC_ENV=development